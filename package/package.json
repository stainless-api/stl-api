{
  "name": "@stl-api/client",
  "version": "0.0.1-alpha.5",
  "description": "primary client package for stl-api",
  "author": "dev@stainlessapi.com",
  "license": "ISC",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "clean": "rimraf dist *.tsbuildinfo",
    "test": "VITE_CJS_TRACE=true vitest watch",
    "prebuild": "mkdir -p dist/esm && echo '{\"type\": \"module\"}' > dist/esm/package.json",
    "prepublishOnly": "npm run clean && npm run build && npm test"
  },
  "keywords": [
    "stainless",
    "api"
  ],
  "dependencies": {
    "stainless": "github:stainless-api/stl-api#stainless-0.1.3"
  },
  "devDependencies": {
    "@tanstack/react-query": "^5.28.0",
    "vitest": "^1.3.1"
  },
  "peerDependencies": {
    "@tanstack/react-query": "~5.x"
  }
}
